class MyHashSet {
    boolean[] array  ;

    /** Initialize your data structure here. */
    public MyHashSet() {
        array  = new boolean[1000001];
    }

    public void add(int i) {
        array[i] = true;
    }

    public void remove(int i) {
        array[i] = false;
    }

    /** Returns true if this set contains the specified element */
    public boolean contains(int i) {
        return array[i];
    }
}
